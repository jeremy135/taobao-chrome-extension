{
  "manifest_version": 3,
  "name": "Taobao in rubles",
  "version": "0.1.0",
  "default_locale": "en",
  "description": "Extension for convert cost from RMB to RUB",
  "author": "blackgrow@yandex.ru",
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "https://*.taobao.com/*",
        "https://*.1688.com/*"
      ]
    }
  ]
}
